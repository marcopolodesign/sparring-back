{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "reservation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::reservation.reservation",
      "inversedBy": "transactions"
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "transactions"
    },
    "amount": {
      "type": "decimal"
    },
    "payment_method": {
      "type": "enumeration",
      "enum": [
        "efectivo",
        "tarjeta",
        "transferencia",
        "gateway-mp",
        "gateway-stripe",
        "dolar"
      ]
    },
    "date": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Completed",
        "Failed",
        "Refunded",
        "Cancelled"
      ]
    },
    "discounts": {
      "type": "decimal"
    },
    "source": {
      "type": "enumeration",
      "enum": [
        "app",
        "front-web",
        "sparring-club",
        "mostrador",
        "whatsapp"
      ]
    },
    "notes": {
      "type": "text"
    },
    "payment_details": {
      "type": "json"
    },
    "seller": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "sales"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "venue": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::court.court"
    },
    "abono": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::abono.abono"
    },
    "originals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction"
    },
    "logs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::log-entry.log-entry",
      "mappedBy": "transaction"
    }
  }
}
