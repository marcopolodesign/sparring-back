{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "friends_added": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "friends_received"
    },
    "friends_received": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "friends_added"
    },
    "coach": {
      "type": "boolean"
    },
    "tournaments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tournament.tournament",
      "mappedBy": "participants"
    },
    "attributes": {
      "displayName": "attributes",
      "type": "component",
      "repeatable": true,
      "component": "attributes.attributes"
    },
    "date_of_birth": {
      "type": "date"
    },
    "document": {
      "type": "biginteger"
    },
    "phone": {
      "type": "string"
    },
    "matches": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::match.match",
      "mappedBy": "members"
    },
    "expo_pushtoken": {
      "type": "string"
    },
    "nivel": {
      "type": "string"
    },
    "court_pos": {
      "type": "string"
    },
    "good_hand": {
      "type": "string"
    },
    "club": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::club.club",
      "inversedBy": "users_permissions_users"
    },
    "reservations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reservation.reservation",
      "mappedBy": "owner"
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "client"
    },
    "sales": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "seller"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "level": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::player-level.player-level"
    },
    "zones": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::zone.zone"
    },
    "match_history": {
      "type": "string"
    },
    "wins": {
      "type": "integer"
    },
    "looses": {
      "type": "integer"
    },
    "best_streak": {
      "type": "integer"
    },
    "current_streak": {
      "type": "integer"
    },
    "hasLogged": {
      "type": "boolean",
      "default": false
    },
    "matches_won": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::match.match",
      "mappedBy": "winners"
    },
    "matches_lost": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::match.match",
      "mappedBy": "losers"
    },
    "played_with_by": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "played_with"
    },
    "played_with": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "played_with_by"
    },
    "visited_clubs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::club.club"
    },
    "is_guest": {
      "type": "boolean"
    }
  }
}
